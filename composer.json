{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "24slides/laravel-saml2": "^2.0",
        "archtechx/enums": "^0.2.0",
        "blade-ui-kit/blade-heroicons": "^1.3",
        "blade-ui-kit/blade-ui-kit": "^0.4",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "graham-campbell/markdown": "^15.0",
        "guzzlehttp/guzzle": "^7.3",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.0",
        "laravel/pint": "^1.0",
        "laravel/prompts": "^0.1.13",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^3.0",
        "mhor/php-mediainfo": "^5.4",
        "opensearch-project/opensearch-php": "^2.2",
        "pbmedia/laravel-ffmpeg": "^8.0",
        "predis/predis": "^1.1",
        "spatie/array-to-xml": "^3.0",
        "te7a-houdini/laravel-trix": "^2.0.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.10",
        "brianium/paratest": "^7.1",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.8",
        "laravel/envoy": "^2.8",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.2",
        "pestphp/pest": "^2.2",
        "pestphp/pest-plugin-drift": "^2.4",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.1",
        "pestphp/pest-plugin-livewire": "^2.0",
        "phpstan/phpstan": "^1.8.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
